# This is Git's per-user configuration file.
[core]
	excludesfile = ~/.gitignore_global
[init]
	defaultBranch = main
[alias]
	sw = switch
	ci = commit
	st = status
	pu = push -u origin HEAD
	br = branch
	unstage = restore --staged
	co = checkout
        pt = push --tags
[pull]
	rebase = false
[push]
	autoSetupRemote = true
[fetch]
	prune = true

